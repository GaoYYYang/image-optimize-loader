{
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "To specify the image file output path."
    },
    "esModule": {
      "type": "boolean"
    },
    "inline": {
      "type": "object",
      "properties": {
        "symbol": {
          "type": "string",
          "description": "Symboled image will be base64 encoded. Default with __inline"
        },
        "antiSymbol": {
          "type": "string",
          "description": "Symboled image will be base64 encoded. Default with __antiInline"
        },
        "limit": {
          "description": "Enables/Disables transformation target file into base64 URIs. Default with 5000",
          "type": ["boolean", "number", "string"]
        },
        "encoding": {
          "description": "Specify the encoding which the file will be inlined with. Default with base64",
          "oneOf": [
            {
              "type": "boolean"
            },
            {
              "enum": [
                "utf8",
                "utf16le",
                "latin1",
                "base64",
                "hex",
                "ascii",
                "binary",
                "ucs2"
              ]
            }
          ]
        },
        "mimetype": {
          "description": "The MIME type for the file to be transformed.",
          "oneOf": [
            {
              "type": "boolean"
            },
            {
              "type": "string"
            }
          ]
        },
        "generator": {
          "description": "Adding custom implementation for encoding files.",
          "instanceof": "Function"
        },
        "disable": {
          "type": "boolean"
        }
      }
    },
    "compress": {
      "type": "object",
      "properties": {
        "mode": {
          "enum": ["lossless", "high", "low"]
        },
        "mozjpeg": {
          "additionalProperties": true,
          "oneOf": [
            {
              "type": "object"
            },
            {
              "type": "boolean"
            }
          ]
        },
        "optipng": {
          "additionalProperties": true,
          "oneOf": [
            {
              "type": "object"
            },
            {
              "type": "boolean"
            }
          ]
        },
        "pngquant": {
          "additionalProperties": true,
          "oneOf": [
            {
              "type": "object"
            },
            {
              "type": "boolean"
            }
          ]
        },
        "gifsicle": {
          "additionalProperties": true,
          "oneOf": [
            {
              "type": "object"
            },
            {
              "type": "boolean"
            }
          ]
        },
        "webp": {
          "additionalProperties": true,
          "oneOf": [
            {
              "type": "object"
            },
            {
              "type": "boolean"
            }
          ]
        },
        "svgo": {
          "additionalProperties": true,
          "oneOf": [
            {
              "type": "object"
            },
            {
              "type": "boolean"
            }
          ]
        },
        "disableOnDevelopment": {
          "type": "boolean"
        },
        "disable": {
          "type": "boolean"
        }
      }
    }
  },
  "additionalProperties": false
}
